pi_from_sfs_array <- function(sfs) {
n = length(sfs) * 2
# Ensure SFS is a named vector or list (excluding monomorphic sites)
i_vals <- 1:(n/2)  # Minor allele frequencies in folded SFS
pi <- sum(2 * i_vals * (n - i_vals) * sfs) / (n * (n - 1))
return(pi)
}
